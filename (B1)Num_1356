import sys
input = sys.stdin.readline

N = input().rstrip()
a = [int(i) for i in N]
if len(a) == 1:
    print("NO")
else:
    for i in range(len(N)):
        b = a[:i+1]
        c = a[i+1:]
        d = 1
        e = 1
        for j in b:
            d *= j
        for j in c:
            e *= j
        if d == e:
            f =1
            break
        else:
            f = 0
if f == 1:
    print("YES")
else:
    print("NO")
