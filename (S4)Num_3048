N1, N2 = map(int, input().split())
a = input()
b = input()
T = int(input())
c = []
for i in range(N2):
    c.append(b[i])

for i in range(N1):
    if T <= N2:
        c.insert(T, a[i])
        if T == 0:
            pass
        else:
            T -= 1
    else:
        if T <= N2:
            c.insert(T, a[i])
            T -=1
        else:
            c.insert(N2, a[i])
            T -= 1
for i in c:
    print(i, end="")
