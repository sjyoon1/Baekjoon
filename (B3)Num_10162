import sys
input = sys.stdin.readline

T = int(input())
a = [0, 0, 0]
if T % 10 != 0:
    print(-1)
else:
    while T != 0:
        if T >= 300:
            T -= 300
            a[0] += 1
        elif T >= 60:
            T -= 60
            a[1] += 1
        else:
            T -= 10
            a[2] += 1
    print(*a)
