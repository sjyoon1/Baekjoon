import sys
input = sys.stdin.readline

a = int(input())
for i in range(a):
    b = list(map(int, input().split()))
    c = 0
    if b[0] + b[4] <= 1:
        c += 1
    else:
        c += (b[0] + b[4])
    if b[1] + b[5] <= 1:
        c += 5
    else:
        c += (5 * (b[1] + b[5]))
    if b[2] + b[6] <= 0:
        pass
    else:
        c += (2 * (b[2] + b[6]))
    c += (2 * (b[3] + b[7]))
    print(c)
