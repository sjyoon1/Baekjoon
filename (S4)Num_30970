import sys
input = sys.stdin.readline

N = int(input())
a = []
b = []
for i in range(N):
    m = list(map(int, input().split()))
    a.append(m)
    b.append([m[1], m[0]])
a.sort(key=lambda x: (x[0], -x[1]), reverse=True)
b.sort(key=lambda x: (-x[0], x[1]), reverse=True)
print(*a[0], *a[1])
print(b[0][1], b[0][0], b[1][1], b[1][0])
