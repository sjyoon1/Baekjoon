import sys
input = sys.stdin.readline

while True:
    n = int(input())
    if n == -1:
        break
    a = []
    for i in range(1, n):
        if n % i == 0:
            a.append(i)
        else:
            pass
    if sum(a) == n:
        print(n, "= ", end="")
        print(*a, sep=" + ")
    else:
        print(n, "is NOT perfect.")
