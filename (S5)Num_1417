import sys
input = sys.stdin.readline

N = int(input())
d = int(input())
a = []
b = 0
for i in range(N-1):
    c = int(input())
    a.append(c)
a.sort(reverse=True)
if N == 1:
    print(0)
else:
    while True:
        if d > a[0]:
            break
        d += 1
        a[0] -= 1
        b += 1
        a.sort(reverse=True)
    print(b)
