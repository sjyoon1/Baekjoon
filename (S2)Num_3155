import sys
input = sys.stdin.readline

N = int(input())
topy = list(map(int, input().split()))
boty = list(map(int, input().split()))
a = [0] 
for i in range(N-1):
    if a[i] <= boty[i+1]:
        a.append(boty[i+1]+1)
    elif a[i] >= topy[i+1]:
        a.append(topy[i+1]-1)
    elif a[i] > boty[i+1] and a[i] < topy[i+1]:
        a.append(a[i])
a.append(0)
a.pop(0)
print(*a)
