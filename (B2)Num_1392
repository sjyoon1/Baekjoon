import sys
input = sys.stdin.readline

N, Q = map(int, input().split())
a = []
for i in range(N):
    b = int(input())
    a.append(b)
for i in range(Q):
    c = int(input())
    d = a[0]
    for j in range(1, N+1):
        if c >= d:
            d += a[j]
        else:
            print(j)
            break
