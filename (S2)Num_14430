import sys
input = sys.stdin.readline

N, M = map(int, input().split())
W = []
for i in range(N):
    a = list(map(int, input().split()))
    W.append(a)
dp = [[0]*M for i in range(N)]
for i in range(N):
    for j in range(M):
        dp[i][j] = W[i][j] + max(dp[i-1][j], dp[i][j-1])
print(dp[N-1][M-1])
