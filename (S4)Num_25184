import sys
input = sys.stdin.readline

N = int(input())
a = [i for i in range(1, N+1)]
if N % 2 == 0:
    b = a[:N//2]
    c = a[N//2:]
    for i in range(N//2-1, -1, -1):
        print(b[i], c[i], end=" ")
else:
    a = a[1:]
    b = a[:(N//2)]
    c = a[(N//2):]
    print(1, end=" ")
    for i in range(N//2-1, -1, -1):
        print(b[i], c[i], end=" ")
    
