a = input()
b = 1
for i in range(len(a)):
    if a[i] == "A":
        if b == 1:
            b = 2
        elif b == 2:
            b = 1
        else:
            pass
    elif a[i] == "B":
        if b == 2:
            b = 3
        elif b == 3:
            b = 2
        else:
            pass
    else:
        if b == 3:
            b = 1
        elif b == 1:
            b = 3
        else:
            pass
print(b)
