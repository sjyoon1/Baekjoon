import sys
input = sys.stdin.readline

R, C = map(int, input().split())
a = []
for i in range(R):
    b = list(map(int, input().split()))
    a.append(b)
T = int(input())
c = []
for i in range(R-2):
    for j in range(C-2):
        d = []
        for k in range(3):
            for l in range(3):
                d.append(a[i+k][j+l])
        d.sort()
        c.append(d[4])
e = 0
for i in c:
    if i >= T:
        e += 1
print(e)
