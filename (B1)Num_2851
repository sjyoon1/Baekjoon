b = 0
d = []
e = []
for i in range(10):
    a = int(input())
    b += a
    if b < 100:
        d.append(b)
    else:
        e.append(b)
if len(d) == 0:
    print(e[0])
elif len(e) == 0:
    print(d[9])
else :
    f = d[len(d)-1]
    g = e[0]
    if g - 100 > 100 - f:
        print(f)
    elif g -100 == 100-f:
        print(g)
    else :
        print(g)
