import sys
input = sys.stdin.readline

N = int(input())
a = []
for i in range(N):
    b = int(input())
    a.append(b)

d = [1] * N
d[0] = a[0]
for i in range(1, N):
    for j in range(i):
        if a[j] < a[i]:
            d[i] = max(d[i], d[j] + a[i])
        else:
            d[i]=max(d[i], a[i])
print(max(d))
