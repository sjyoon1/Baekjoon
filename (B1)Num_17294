import sys
input = sys.stdin.readline

k = input().rstrip()
if len(k) <= 2:
    print("◝(⑅•ᴗ•⑅)◜..°♡ 뀌요미!!")
else:
    a = int(k[1]) - int(k[0])
    b = 0
    for i in range(len(k)-1):
        if int(k[i+1]) - int(k[i]) == a:
            pass
        else:
            b = 1
            break
    if b == 0:
        print("◝(⑅•ᴗ•⑅)◜..°♡ 뀌요미!!")
    else:
        print("흥칫뿡!! <(￣ ﹌ ￣)>")
