import sys
input = sys.stdin.readline

N = int(input())
a = list(map(int, input().split()))
a.sort()
if N % 2 == 1:
    print(a[N//2])
else:
    b = 0
    c = 0
    for i in range(N):
        if a[N//2] - a[i] < 0:
            b -= (a[N//2] - a[i])
        else:
            b += (a[N//2] - a[i])
        if a[(N//2)-1] - a[i] < 0:
            c -= (a[(N//2)-1] - a[i])
        else:
            c += (a[(N//2)-1] - a[i])
    if b < c:
        print(a[N//2])
    else:
        print(a[(N//2)-1])
