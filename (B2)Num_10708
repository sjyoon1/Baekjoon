import sys
input = sys.stdin.readline

N = int(input())
M = int(input())
A = list(map(int, input().split()))
fr = [0 for i in range(N)]
for i in range(M):
    B = list(map(int, input().split()))
    for j in range(N):
        if B[j] == A[i]:
            fr[j] += 1
    fr[A[i]-1] += (N - B.count(A[i]))
for i in fr:
    print(i)
