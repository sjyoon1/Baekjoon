import sys
input = sys.stdin.readline

N = int(input())
a = 1
c = [2**i for i in range(33)]
while True:
    if N in c:
        print(32-c.index(N)-1)
        break
    else:
        while True:
            a *= 2
            if a >= N:
                a //= 2
                N -= a
                a = 1
                break
            else:
                continue
